{
  "name": "sectors-without-number",
  "version": "6.5.0",
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mpigsley/sectors-without-number"
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "start": "npm-run-all -p watch:css start:js",
    "start:js": "react-scripts start",
    "start:prod": "sh -ac '. .env.prod; npm start'",
    "build": "npm run build:css && react-scripts build",
    "build:css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch:css": "npm run build:css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "test": "react-scripts test --env=jsdom --coverage",
    "test:watch": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "deploy": "npm run build && firebase deploy",
    "deploy:prod": "sh -ac '. .env.prod; npm run deploy'",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "translate": "node ./src/utils/convert-translations.js"
  },
  "dependencies": {
    "chance": "^1.0.16",
    "classnames": "^2.2.6",
    "connected-react-router": "^4.4.0",
    "copy-to-clipboard": "^3.0.8",
    "dayjs": "^1.6.10",
    "firebase": "^5.3.0",
    "history": "^4.7.2",
    "is-url-external": "^1.0.3",
    "lodash": "^4.17.10",
    "node-sass-chokidar": "1.3.0",
    "npm-run-all": "^4.1.3",
    "pluralize": "^7.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-color": "^2.14.1",
    "react-dom": "^16.4.1",
    "react-fastclick": "^3.0.2",
    "react-feather": "^1.1.0",
    "react-hint": "^3.1.0",
    "react-intl": "^2.4.0",
    "react-loadable": "^5.4.0",
    "react-markdown": "^3.3.4",
    "react-modal": "^3.4.5",
    "react-redux": "^5.0.7",
    "react-redux-toastr": "^7.3.0",
    "react-router-dom": "^4.3.1",
    "react-select": "^1.2.1",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^3.0.1",
    "tinycolor2": "^1.4.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "enzyme": "^3.1.1",
    "enzyme-adapter-react-16": "^1.0.4",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.1",
    "eslint-plugin-react": "^7.10.0",
    "firebase-tools": "^4.0.0",
    "prettier": "^1.13.7",
    "react-scripts": "^1.0.1",
    "react-test-renderer": "^16.4.1",
    "redux-devtools-extension": "^2.13.5",
    "source-map-explorer": "^1.5.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!node_modules",
      "!src/components/**/index.js"
    ]
  }
}

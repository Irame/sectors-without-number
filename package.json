{
  "name": "sectors-without-number",
  "version": "1.0.0",
  "private": true,
  "eslintConfig": {
    "extends": "airbnb"
  },
  "scripts": {
    "start": "npm-run-all -p watch:css start:js",
    "start:js": "NODE_PATH=./src react-scripts start",
    "start:prod": "NODE_PATH=./src npm run build && firebase serve",
    "build": "npm run build:css && NODE_PATH=./src react-scripts build",
    "build:css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch:css": "npm run build:css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "test": "NODE_PATH=./src react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "deploy": "npm run build && firebase deploy",
    "lint": "eslint .",
    "lint:fix": "eslint --fix ."
  },
  "dependencies": {
    "chance": "^1.0.10",
    "classnames": "^2.2.5",
    "copy-to-clipboard": "^3.0.7",
    "firebase-tools": "^3.9.1",
    "history": "^4.6.3",
    "is-url-external": "^1.0.3",
    "lodash": "^4.17.4",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.0.2",
    "prop-types": "^15.5.10",
    "query-string": "^5.0.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-feather": "^1.0.5",
    "react-redux": "^5.0.5",
    "react-redux-toastr": "^7.1.0",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-react": "^7.1.0",
    "prettier": "^1.5.3",
    "react-scripts": "^1.0.10"
  }
}
